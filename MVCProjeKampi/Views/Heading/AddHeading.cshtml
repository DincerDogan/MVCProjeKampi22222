@model EntityLayer.Concreate.Heading

@{
    ViewBag.Title = "AddHeading";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />


@using (Html.BeginForm("AddHeading", "Heading", FormMethod.Post))
{
<div class="form form-group">
    @Html.Label("Heading Name ")
    @Html.TextBoxFor(x => x.HeadingName, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.HeadingName)
    <br />
    @Html.Label("Category")
    @Html.DropDownListFor(x => x.CategoryId, (List<SelectListItem>)ViewBag.dgr, new { @class = "form-control" })
    <br />
   


    <br />

    <button class="btn btn-success" id="btn1">Baslıgı Olustur</button>

</div>
}

<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<script src="~/AdminLTE-3.0.4/plugins/jquery/jquery.min.js"></script>
<script src="~/AdminLTE-3.0.4/plugins/jquery/jquery.js"></script>
<script src="~/AdminLTE-3.0.4/plugins/jquery/jquery.slim.min.js"></script>
<script>
    $('#btn1').click(function () {
        swal("Tebrikler!", "Başarılı bir şekilde eklendi.", "success");

    })
</script>





